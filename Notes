sonar_dict = {}

if response.status_code == 200:
    jenkinsfile_content = response.text
    
    # Regular expression to find sonar parameters and their values
    matches = re.findall(r'(\b(?:project\.sonarkey|sonarkey|sonarname)\b)\s*=\s*["\'](.+?)["\']', jenkinsfile_content, re.IGNORECASE)

    if matches:
        for param, value in matches:
            if param not in sonar_dict:  # Store only the first occurrence
                sonar_dict[param] = value
            
        print("Extracted SonarQube Parameters:", sonar_dict)
    else:
        print("No SonarQube parameters found in Jenkinsfile.")
else:
    print(f"Failed to fetch Jenkinsfile: {response.status_code}, {response.text}")